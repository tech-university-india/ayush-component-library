{
  "name": "ayush-component-library",
  "version": "1.1.17",
  "description": "A React Component Library",
  "author": "Kushagra Ved Nigam",
  "license": "MIT",
  "repository": "Kushagraved/kushu-lib-tailwind",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/components/index.tsx",
  "types": "dist/src/components/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "NODE_ENV=publish microbundle-crl --no-compress --format modern,cjs",
    "start": "NODE_ENV=development microbundle-crl watch --no-compress --format modern,cjs ",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "preinstall": "npm set force true"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.3",
    "@types/react-dom": "^18.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "autoprefixer": "^9.0.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-standard": "^4.1.0",
    "gh-pages": "^5.0.0",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-app-rewire-alias": "^1.1.7",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-scripts": "^5.0.1",
    "react-spinners": "^0.13.8",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^5.0.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^1.4.0",
    "classnames": "^2.3.2"
  }
}
